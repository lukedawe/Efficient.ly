{
    "name": "Efficient.ly",
    "description": "A Chrome extension to remove distractions and make sure you're working efficiently!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
         persistent:true

      },
      "options_page": "options.html",
      "permissions": ["storage", "activeTab", "scripting", "tabs"],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/default_16.png",
            "32": "/images/default_32.png",
            "48": "/images/default_48.png",
            "128": "/images/default_128.png"
          }
      },
      "icons": {
        "16": "/images/default_16.png",
        "32": "/images/default_32.png",
        "48": "/images/default_48.png",
        "128": "/images/default_128.png"
      }
  }
